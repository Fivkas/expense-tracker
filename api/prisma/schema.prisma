datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Expense {
  id        Int      @id @default(autoincrement())
  title     String
  amount    Float
  category  Category @relation(fields: [categoryId], references: [id])
  categoryId Int
  createdAt DateTime @default(now())
}

model Category {
  id      Int       @id @default(autoincrement())
  name    String
  expenses Expense[]
}

